# bestguess module

BESTGUESS_DIRPATH = $(COVERED_DIRPATH)/cache/bestguess

# NOTE: NO need to add BESTGUESS_INCDIR, which is the same as COVERED_INCDIR

BESTGUESS_SRCFILES = $(wildcard $(BESTGUESS_DIRPATH)/*.c)
BESTGUESS_OBJECTS += $(BESTGUESS_SRCFILES:.c=.o)
BESTGUESS_SHARED_OBJECTS += $(BESTGUESS_SRCFILES:.c=.shared.o)
DEPS += $(BESTGUESS_SRCFILES:.c=.d)
CLEANS += $(BESTGUESS_OBJECTS) $(BESTGUESS_SHARED_OBJECTS)
LINK_OBJECTS += $(BESTGUESS_OBJECTS)

# NOTE: NO need to add BESTGUESS_LDDIR and BESTGUESS_LDLIBS due to no library